{
  "name": "musicuon",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "dev:audio-player": "turbo run dev --filter=@musicuon/audio-player --concurrency=50",
    "dev:play-list": "turbo run dev --filter=@musicuon/play-list --concurrency=50",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@8.6.10",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}